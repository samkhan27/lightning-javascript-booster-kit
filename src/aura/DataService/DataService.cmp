<aura:component extends="c:ApplicationBase" Controller="DataServiceProvider">

	<!--Methods-->
	<aura:method name="executeQuery" action="{!c.executeQueryJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="getRecord" action="{!c.getRecordJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="getRecords" action="{!c.getRecordsJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="getFieldSetData" action="{!c.getFieldSetDataJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="createRecord" action="{!c.createRecordJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="updateRecord" action="{!c.updateRecordJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>

	<aura:method name="deleteRecord" action="{!c.deleteRecordJS}">
		<aura:attribute name="params" type="Object"/>
	</aura:method>
	
</aura:component>